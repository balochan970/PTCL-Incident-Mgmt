    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.balochan970.ptclincidentmobilenew"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.balochan970.ptclincidentmobilenew"
    },
    "extra": {
      "eas": {}
    },
    "owner": "balochan970",
    "plugins": [
      "expo-dev-client"
    ]
  }
} 